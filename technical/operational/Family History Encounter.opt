            <term_definitions code="at0000">
              <items id="description">Summary information about the problems and/or diagnoses found in family members.</items>
              <items id="text">Family History</items>
            </term_definitions>
            <term_definitions code="at0001">
              <items id="description">@ internal @</items>
              <items id="text">Tree</items>
            </term_definitions>
            <term_definitions code="at0002">
              <items id="comment">Use this field to capture unstructured family history information recorded in clinical records or to import textual data from existing/legacy clinical systems.</items>
              <items id="description">Narrative overview about problems, diagnoses, and genetic markers present in family members.</items>
              <items id="text">Summary</items>
            </term_definitions>
            <term_definitions code="at0003">
              <items id="description">Details about a specific family member.</items>
              <items id="text">Per family member</items>
            </term_definitions>
            <term_definitions code="at0004">
              <items id="comment">For privacy reasons this may not be appropriate for recording, sharing or public display and in this situation the 'Alias' should be used.</items>
              <items id="description">Name of family member.</items>
              <items id="text">Name</items>
            </term_definitions>
            <term_definitions code="at0005">
              <items id="comment">At least a partial date will be useful to calculate an age. For example, current age of relative in a pedigree chart.</items>
              <items id="description">Date of birth of the family member.</items>
              <items id="text">Date of birth</items>
            </term_definitions>
            <term_definitions code="at0008">
              <items id="description">Detail about problems or diagnoses for the family member.</items>
              <items id="text">Medical History</items>
            </term_definitions>
            <term_definitions code="at0009">
              <items id="comment">Coding of the problem or diagnosis with a terminology is preferred, where possible. May link from this data element to a detailed record of a Problem/Diagnosis using the EVALUATION.problem_diagnosis archetype with the Subject of Care set to the family member, not to the patient.</items>
              <items id="description">Identification of the family member's problem or diagnosis.</items>
              <items id="text">Problem/Diagnosis</items>
            </term_definitions>
            <term_definitions code="at0010">
              <items id="description">Estimated or actual age of the family member when the problem/diagnosis was clinically recognised.</items>
              <items id="text">Age at Onset</items>
            </term_definitions>
            <term_definitions code="at0011">
              <items id="description">Age of the family member at death.</items>
              <items id="text">Age at death</items>
            </term_definitions>
            <term_definitions code="at0012">
              <items id="description">Narrative description or comments about clinical aspects of the family member's problem/diagnosis.</items>
              <items id="text">Clinical Description</items>
            </term_definitions>
            <term_definitions code="at0014">
              <items id="description">True if the problem/diagnosis was a direct cause of death in this family member.</items>
              <items id="text">Cause of Death?</items>
            </term_definitions>
            <term_definitions code="at0016">
              <items id="comment">Coding of the relationship with a terminology is preferred, where possible and including specification of maternal and paternal as required.</items>
              <items id="description">The relationship of the family member to the subject of care.</items>
              <items id="text">Relationship</items>
            </term_definitions>
            <term_definitions code="at0020">
              <items id="comment">To be used to assist in distinguishing one individual from multiple family members with identical relationships. For example, the label to distinguish one specific sister from three known sisters might be 'eldest sister' or 'sister #1'.</items>
              <items id="description">An alternative name or label to uniquely identify a family member, without using a personal name which might publicly identify the individual.</items>
              <items id="text">Alias</items>
            </term_definitions>
            <term_definitions code="at0022">
              <items id="description">Description of risk-related genetic markers identified in this family member.</items>
              <items id="text">Marker Description</items>
            </term_definitions>
            <term_definitions code="at0023">
              <items id="comment">Record as 'True' if family member is deceased.</items>
              <items id="description">Is the family member is deceased?</items>
              <items id="text">Deceased?</items>
            </term_definitions>
            <term_definitions code="at0024">
              <items id="comment">Note: More data elements will be needed in future to record detailed genetic marker information.</items>
              <items id="description">Detailed genetic marker information for the family member.</items>
              <items id="text">Genetic Markers</items>
            </term_definitions>
            <term_definitions code="at0025">
              <items id="description">@ internal @</items>
              <items id="text">Tree</items>
            </term_definitions>
            <term_definitions code="at0026">
