              <items id="text">Last Updated</items>
            </term_definitions>
            <term_definitions code="at0027">
              <items id="description">Details about the relative's genetic markers.</items>
              <items id="text">Genetic Marker Details</items>
            </term_definitions>
            <term_definitions code="at0028">
              <items id="description">Details about the conditions that occur in the subject's family.</items>
              <items id="text">Per Condition</items>
            </term_definitions>
            <term_definitions code="at0029">
              <items id="comment">Coding of the index condition with a terminology is preferred, where possible.</items>
              <items id="description">Name of the disease, illness, condition or problem.</items>
              <items id="text">Index condition name</items>
            </term_definitions>
            <term_definitions code="at0030">
              <items id="description">Narrative description about the index condition's occurrence in family members.</items>
              <items id="text">Description</items>
            </term_definitions>
            <term_definitions code="at0031">
              <items id="description">The number of family members affected by the index condition.</items>
              <items id="text">Affected family</items>
            </term_definitions>
            <term_definitions code="at0032">
              <items id="description">The proximity or line of the family member.</items>
              <items id="text">Relationship</items>
            </term_definitions>
            <term_definitions code="at0033">
              <items id="description">Biological sex of the relative/s.</items>
              <items id="text">Sex</items>
            </term_definitions>
            <term_definitions code="at0034">
              <items id="description">The number of family members affected by the index condition.</items>
              <items id="text">Number affected</items>
            </term_definitions>
            <term_definitions code="at0035">
              <items id="description">The number of family members unaffected by the index condition.</items>
              <items id="text">Number unaffected</items>
            </term_definitions>
            <term_definitions code="at0036">
              <items id="comment">Record as 'True' if the index condition has been identified in genetic family members.</items>
              <items id="description">There is a positive family history of the index condition.</items>
              <items id="text">Present?</items>
            </term_definitions>
            <term_definitions code="at0037">
              <items id="description">50% genetic share with the subject - parent, sibling or child.</items>
              <items id="text">First degree relative</items>
            </term_definitions>
            <term_definitions code="at0038">
              <items id="description">25% genetic share with the subject - grandparent, aunt, uncle, niece, nephew and grandchildren.</items>
              <items id="text">Second degree relative</items>
            </term_definitions>
            <term_definitions code="at0039">
              <items id="description">12.5% genetic share with the subject - great grandparent, great aunt, great uncle, first cousin, children of nieces and nephews, and great grandchildren.</items>
              <items id="text">Third degree relative</items>
            </term_definitions>
            <term_definitions code="at0040">
              <items id="description">Related through the subject's mother.</items>
              <items id="text">Maternal relative</items>
            </term_definitions>
            <term_definitions code="at0041">
              <items id="description">Related through the subject's father.</items>
              <items id="text">Paternal relative</items>
            </term_definitions>
            <term_definitions code="at0042">
              <items id="description">Entire genetic family of the subject.</items>
              <items id="text">Family</items>
            </term_definitions>
